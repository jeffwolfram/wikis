<h1>Add a Collaborator to your wiki</h1>
<div class="container">
  <div class="row">
    <div class="col-md-8">
      <% @users.each do |user| %>
    <h3>  <%= button_to "Make Admin", {action: "make_admin", id: current_user.id }, method: :put %>
      <%= link_to user.email%> role: <strong><%= current_user.role %></strong></h3>
      <% end %>
    </div>
  </div>
</div>
